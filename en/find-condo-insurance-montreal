      })
        function closeAdBlockerEventTracking() {
        $.ajax('api/v1/close-ad-blocker-event-tracking',
        {
          method: 'GET',
            dataType: 'json', // type of response data
            success: function (data,status,xhr) {   // success callback function
              console.log('closeAdBlockerEventTracking result: ', data);
            },
            error: function (jqXhr, textStatus, errorMessage) { // error callback
              console.log('closeAdBlockerEventTracking error: ', errorMessage);
            }
        });
        }
