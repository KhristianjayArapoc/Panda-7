<script>
  if ($("#insurance-type-name-switcher").length) {
    var insuranceTypes = [
      "<span data-tid=3950>Auto</span>",
      "<span data-tid=3075>Locataire</span>",
      "<span data-tid=3951>Condo</span>",
      "<span data-tid=3073>Habitation</span>",
    ];
if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
    var insuranceTypes = [
      "<br /><span data-tid=3950>Auto</span>",
      "<br /><span data-tid=3075>Locataire</span>",
      "<br /><span data-tid=3951>Condo</span>",
      "<br /><span data-tid=3073>Habitation</span>",
    ];
}

    $("#insurance-type-name-switcher").html(insuranceTypes[0])
    var index = 1;
    setInterval(function(){
      $("#insurance-type-name-switcher").animate({
        opacity:0
      },function()
        {
          if(index < insuranceTypes.length) {
            $(this).html(insuranceTypes[index]).animate({opacity:1})
            index++;
          }
          else {
            $(this).html(insuranceTypes[0]).animate({opacity:1})
            index = 1;
          }});
    },3500);
  }
</script>
