<script>
      var adblock_modal_is_active = "inactive";
      if(
        window.canRunAds === undefined &&
        adblock_modal_is_active === "active"
      ) {
        // adblocker detected, show fallback
        // Get the modal-blocker
        var modal_blocker = document.getElementById("myModal-blocker");
          modal_blocker.style.display = "block";
        // When the user clicks anywhere outside of the modal-blocker, close it
        window.onclick = function(event) {
          if (event.target == modal_blocker) {
          //  modal_blocker.style.display = "none";
          }
        }
      }
      $(function(){
        $('.adblocker-button').on('click',function(){
          console.log('click')
          $('#adblocker-message').hide();
          $('#adblocker-video').show();
        })

        $('#adblocker-close').on('click',function(){
          $('#myModal-blocker').css('display','none')
          closeAdBlockerEventTracking();
        })
      })
        function closeAdBlockerEventTracking() {
        $.ajax('api/v1/close-ad-blocker-event-tracking',
        {
          method: 'GET',
            dataType: 'json', // type of response data
            success: function (data,status,xhr) {   // success callback function
              console.log('closeAdBlockerEventTracking result: ', data);
            },
            error: function (jqXhr, textStatus, errorMessage) { // error callback
              console.log('closeAdBlockerEventTracking error: ', errorMessage);
            }
        });
        }
      </script>

