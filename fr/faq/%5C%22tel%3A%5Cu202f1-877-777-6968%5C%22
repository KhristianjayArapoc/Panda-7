		<script>
  /**
   *
   */
  function handleCheckConnectionOnlineOffline() {
    window.addEventListener('online', function(e) {
      swal("État Internet restauré", "Vous êtes de retour en ligne", 'success');
    }, false);

    window.addEventListener('offline', function(e) {
      // Queue up events for server.
      swal("État Internet hors ligne", "S'il vous plait, vérifiez votre connexion internet.", 'error');
    }, false);
  }

  $(document).ready(function() {
    //sweetalert
    handleCheckConnectionOnlineOffline();
  });
</script>

